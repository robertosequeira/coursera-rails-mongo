<p id="notice"><%= notice %></p>

<h1>Listing Api Races</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_races.each do |api_race| %>
      <tr>
        <td><%= link_to 'Show', api_race %></td>
        <td><%= link_to 'Edit', edit_api_race_path(api_race) %></td>
        <td><%= link_to 'Destroy', api_race, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Race', new_api_race_path %>
